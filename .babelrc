{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    "@emotion",
    [
      "module-resolver",
      {
        "root": ["./src/"],
        "alias": {
          "@": "./src"
        }
      }
    ]
  ]
}
